<html>
  <head>
    <link rel="stylesheet" href="/styles.css">
    <title><%= categoryName %> Games</title>
  </head>
  <body>
    <div id = "home-container">
      <div id = "form-title-container">
        <h1><%= categoryName %> Games</h1>
        <%- include('forms/newGameForm.ejs', {categoryName: categoryName}) %>
      </div>
      <div id = "game-container">
        <% rows.forEach((row) => { %>
          <div id = "game-button-container">
            <%- include('games/game.ejs', {row: row}); %>
            <%- include('buttons/deleteGameButton.ejs', {categoryName: categoryName, row: row}) %>
          </div> 
        <% }); %>
      </div>
    </div>
  </body>
</html>
